import java.util.*;

class rectangle {

    public static void main(String[] args) {
        int[] arr = {1, 2, 4, 5}; // missing 3
        HashSet<Integer> set = new HashSet<>();
        for (int i = 0; i < arr.length; i++) {
            set.add(arr[i]);
        }
        for (int i = 1; i <= arr.length + 1; i++) {
            if (!set.contains(i)) {
                System.out.println("Missing number is " + i);
                break;
            }
        }
    }
}
